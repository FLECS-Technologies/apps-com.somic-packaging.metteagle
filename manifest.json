{
    "$schema": "https://raw.githubusercontent.com/FLECS-Technologies/app-sdk/main/manifest.schema.json",
    "_schemaVersion": "2.0.0",
    "app": "com.somic-packaging.metteagle",
    "version": "##VERSION##",
    "revision": "0",
    "image": "flecs.azurecr.io/com.somic-packaging.metteagle",
    "multiInstance": false,
    "editor": ":8080",
    "ports": [
        "8080:8080"
    ],
    "volumes": [
        "/home/somic/app/metteagle/:/metteagle"
    ],
    "env": [
        "VideoFileLocation=/app/metteagle/assets",
        "ApplicationSettingsPath=/app/metteagle/metteagle.yaml",
        "ConnectionStrings__SQLiteDefault=DataSource=/app/metteagle/metteagle.db",
        "MetteagleLocation=/app/metteagle/"
    ]
}
